# VSCode Bridge Connector API Key
# Replace 'your-secret-key-here' with your actual secret key
VSCODE_API_KEY=your-secret-key-here

# Bridge HTTP port (matches bridgeConnector.port setting)
BRIDGE_PORT=8282

# Main Panel Configuration (Optional)
# URL to open when "Open Main Panel" menu item is clicked
MAIN_PANEL_URL=http://127.0.0.1:3000

# Display name for the main panel menu item
MAIN_PANEL_TITLE=Main Panel

# Local server port used by the generated main panel Node app
MAIN_PANEL_PORT=3000
